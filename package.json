{
  "name": "react-admin-template",
  "version": "0.1.0",
  "main": "index.js",
  "scripts": {
    "bootstrap": "yarn || npm i",
    "precommit": "npm run lint-staged",
    "dev": "cross-env PORT=8080 umi dev",
    "build": "umi build",
    "build:web": "umi build && cd ./dist && anywhere -h localhost -p 8080",
    "lint-staged": "lint-staged",
    "lint-staged:ts": "npm run tsc",
    "lint:ts": "tslint --project tsconfig.json --format codeFrame",
    "lint:style": "stylelint \"src/**/*.less\" --syntax less",
    "tsc": "tsc",
    "test": "umi test",
    "test:component": "umi test ./src/components",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "deploy": "rm -rf node_modules/gh-pages/.cache && gh-pages -d dist",
    "build:view": "npm run build && cd ./dist && anywhere -p 8080",
    "prettier": "prettier --write ./src/**/**/**/*"
  },
  "repository": "https://github.com/jiumao-fe/react-admin-template.git",
  "author": "wangxingkang <wang_xingkang@qq.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "npm run commitmsg"
    }
  },
  "dependencies": {
    "@antv/data-set": "^0.10.1",
    "antd": "^3.10.7",
    "axios": "^0.18.0",
    "bizcharts": "^3.2.5-beta.4",
    "bizcharts-plugin-slider": "^2.1.1-beta.1",
    "classnames": "^2.2.6",
    "dva": "^2.4.0",
    "lodash": "^4.17.10",
    "lodash-decorators": "^6.0.0",
    "memoize-one": "^4.0.3",
    "moment": "^2.22.2",
    "nprogress": "^0.2.0",
    "numeral": "^2.0.6",
    "qs": "^6.5.2",
    "rc-animate": "^2.4.4",
    "react": "^16.5.2",
    "react-copy-to-clipboard": "^5.0.1",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.5.2",
    "react-intl": "^2.4.0",
    "react-text-mask": "^5.4.3",
    "react-transition-group": "^2.4.0",
    "rollbar": "^2.4.4",
    "store": "^2.0.12",
    "xlsx": "^0.13.5"
  },
  "devDependencies": {
    "@commitlint/cli": "^7.1.1",
    "@jiumao/commitlint-config": "^0.0.2",
    "@types/classnames": "^2.2.6",
    "@types/enzyme": "^3.1.15",
    "@types/jest": "^23.3.9",
    "@types/lodash": "^4.14.116",
    "@types/memoize-one": "^3.1.1",
    "@types/node": "^10.5.7",
    "@types/nprogress": "^0.0.29",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.4.7",
    "@types/react-copy-to-clipboard": "^4.2.6",
    "@types/react-document-title": "^2.0.2",
    "@types/react-dom": "^16.0.6",
    "@types/react-intl": "^2.3.9",
    "@types/react-text-mask": "^5.4.2",
    "@types/store": "^2.0.0",
    "cross-env": "^5.2.0",
    "enzyme": "^3.7.0",
    "eslint": "^5.4.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-typescript": "^0.12.0",
    "gh-pages": "^2.0.1",
    "husky": "^1.1.3",
    "image-webpack-loader": "^4.3.1",
    "jest": "^23.6.0",
    "lint-staged": "^8.0.4",
    "mockjs": "^1.0.1-beta3",
    "prettier": "^1.14.2",
    "stylelint": "^9.4.0",
    "stylelint-config-prettier": "^3.3.0",
    "stylelint-config-standard": "^18.2.0",
    "svg-sprite-loader": "^3.9.2",
    "tslint": "^5.11.0",
    "tslint-config-jiumao": "^0.0.3",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.4",
    "typescript-eslint-parser": "^18.0.0",
    "umi": "^2.2.6",
    "umi-plugin-react": "^1.2.3"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
